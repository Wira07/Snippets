{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Tag php1": {
		"prefix": "php1",
		"body": "<?= $1 ?>",
		"description": "PHP TAG ALA WIRA SUKMA SAPUTRA"
	},
	"Tag php2": {
		"prefix": "phpe",
		"body": "<?= echo $1 ?>",
		"description": "PHP TAG ALA WIRA SUKMA SAPUTRA WITH ECHO"
	},
	"Inline Echo": {
		"prefix": "phpp",
		"body": "<?= $; ?>",
		"description": "PHP TAG ALA WIRA SUKMA SAPUTRA WITH ECHO AND SEMICOLON"
	},
	"Tag php awal": {
		"prefix": "php",
		"body": "<?php ?>",
		"description": "PHP TAG ALA WIRA SUKMA SAPUTRA WITH ECHO AND SEMICOLON AND QUOTES"
	},
	"Input Label": {
		"prefix": "formel",
		"body": [
			"<label for=\"${2:username}\">${3:Username}</label>",
			"<input type=\"${1:text}\" name=\"${2:username}\" id=\"${2:username}\">"
		]
	},
	"Foreach": {
		"prefix": "foreach",
		"body": [
			"<?php foreach ($$mahasiswa as $$mhs) : ?>",
			"<?php endforeach; ?>"
		]
	},
	// "isset": {
	// 	"prefix": "isset",
	// 	"body": "<?php if (isset($_POST["submit"])) : ?>"
	// },

	"myform": {
		"prefix": "myform",
		"body": "<form method=\"${1|get,post|}\" action=\"$2\">$3</form>"
	},
}


